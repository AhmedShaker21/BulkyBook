@model IEnumerable<BulkyBook.Models.ActivityLog>

<h2>سجل الأنشطة</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>المذكرة</th>
            <th>المستخدم</th>
            <th>الإجراء</th>
            <th>من حالة</th>
            <th>إلى حالة</th>
            <th>التاريخ</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var l in Model)
    {
        <tr>
            <td>@l.MemoId</td>
            <td>@l.UserId</td>
            <td>@l.Action</td>
            <td>@l.OldStatus</td>
            <td>@l.NewStatus</td>
            <td>@l.CreatedAt.ToString("yyyy/MM/dd HH:mm")</td>
        </tr>
    }
    </tbody>
</table>
